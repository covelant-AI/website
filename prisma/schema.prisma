generator client {
  provider = "prisma-client-js"
  output   = "./"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Inquiry {
  id          String   @id @default(uuid())
  name        String
  email       String
  message     String
  companyName String?
  inquiryType String
  createdAt   DateTime @default(now())
}
